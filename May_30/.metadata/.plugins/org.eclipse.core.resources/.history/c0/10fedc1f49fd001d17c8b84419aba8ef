package com.spring.watcha.common;

public class Movie_roleVO {

	private String movie_id;
	private String actor_id;
	private String role;
	private int casting_order;
	
	
	private Movie_roleVO(){};
	
	public Movie_roleVO(String movie_id, String actor_id, String role, int casting_order) {
		 
		this.movie_id = movie_id;
		this.actor_id = actor_id;
		this.role = role;
		this.casting_order = casting_order;
	}

	public String getMovie_id() {
		return movie_id;
	}

	public void setMovie_id(String movie_id) {
		this.movie_id = movie_id;
	}

	public String getActor_id() {
		return actor_id;
	}

	public void setActor_id(String actor_id) {
		this.actor_id = actor_id;
	}

	public String getRole() {
		return role;
	}

	public void setRole(String role) {
		this.role = role;
	}

	public int getCasting_order() {
		return casting_order;
	}

	public void setCasting_order(int casting_order) {
		this.casting_order = casting_order;
	}
	
	
	
}
